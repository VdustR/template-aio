{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "files": ["**/*"],
  "ignorePaths": [
    "**/__snapshots__/**",
    "**/.env.*.local",
    "**/.env.local",
    "**/.gitignore",
    "**/*.svg",
    "/.git",
    "/.prettierignore",
    "/.stylelintignore",
    "/*.code-workspace",
    "/pnpm-lock.yaml"
  ],
  "includeRegExpList": ["[ -~]"],
  "useGitignore": true,
  "version": "0.2",
  "words": ["antfu", "jsdoc", "nums", "nvmrc", "pathe", "tsdoc"]
}
